@{
    ViewData["Title"] = "DeleteMember";
}

<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.RemoveMember.MemberId" class="control-label"></label>
                <input asp-for="@Model.RemoveMember.MemberId" class="form-control" readonly="" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.RemoveMember.FirstName" class="control-label"></label>
                <input asp-for="@Model.RemoveMember.FirstName" class="form-control" readonly="" />
