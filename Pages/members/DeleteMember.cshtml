            <div class="form-group">
                <label asp-for="@Model.RemoveMember.Address" class="control-label"></label>
                <input asp-for="@Model.RemoveMember.Address" class="form-control" readonly="" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.RemoveMember.Mail" class="control-label"></label>
                <input asp-for="@Model.RemoveMember.Mail" class="form-control" readonly="" />
