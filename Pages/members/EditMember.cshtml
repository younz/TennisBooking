@page
@model TennisBooking.Pages.members.EditMemberModel
@{
    ViewData["Title"] = "Index";
}
<h1>New Member</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <input asp-for="@Model.EditMember.MemberId" class="form-control" hidden=""/>
            </div>
            <div class="form-group">
                <label asp-for="@Model.EditMember.FirstName" class="control-label">Fornavn</label>
                <input asp-for="@Model.EditMember.FirstName" class="form-control" required="" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.EditMember.LastName" class="control-label">Efternavn</label>
                <input asp-for="@Model.EditMember.LastName" class="form-control" required="" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.EditMember.Address" class="control-label"></label>
                <input asp-for="@Model.EditMember.Address" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.EditMember.Mail" class="control-label"></label>
                <input asp-for="@Model.EditMember.Mail" class="form-control" required="" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.EditMember.Password" class="control-label"></label>
                <input type="password" asp-for="@Model.EditMember.Password" class="form-control" required="" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.EditMember.PhoneNumber" class="control-label"></label>
                <input asp-for="@Model.EditMember.PhoneNumber" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Edit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-page="Index">Back to List</a>
